<Window x:Class="BinFileUploading.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BinFileUploading"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="Btn_Upload" Content="Button" HorizontalAlignment="Left" Margin="180,60,0,0" VerticalAlignment="Top" Width="75" Click="Btn_Upload_Click"/>
        <Button x:Name="Btn_selectPath" Content="Button" HorizontalAlignment="Left" Margin="85,60,0,0" VerticalAlignment="Top" Width="75" Click="Btn_selectPath_Click"/>

    </Grid>
</Window>
